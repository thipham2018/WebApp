@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div class="text-center">
    <h1 class="display-4">Web App</h1>
    <p>Learn about <a href="https://docs.microsoft.com/aspnet/core">building Web apps with ASP.NET Core and take data from json file</a>.</p>
</div>


<div class="card-columns">
        @foreach (var product in Model.Products)
        {
    <div class="card">
        <div class="card-img" style="background-image: url('@product.Image');"></div>
        <div class="card-body">
            <h5>@product.Title</h5>

        </div>



        @product.Title
    </div>
        }
    </div>